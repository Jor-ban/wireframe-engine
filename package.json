{
  "name": "wireframe_engine",
  "version": "1.0.0",
  "description": "Engine for three.js",
  "main": "index.js",
  "scripts": {
    "dev": "npm start",
    "start": "cross-env NODE_ENV=development concurrently --kill-others \"node ./src/engine/fileManager/index.js\" \"webpack-dev-server\"",
    "dev:watch": "cross-env NODE_ENV=development concurrently --kill-others \"nodemon ./src/engine/fileManager/index.js\" \"webpack-dev-server\"",
    "dev:fileManager-only": "nodemon ./src/engine/fileManager/index.js",
    "dev:frontend-only": "cross-env NODE_ENV=development webpack-dev-server",
    "dev:prod": "cross-env NODE_ENV=production webpack-dev-server",

    "build:dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",

    "precommit": "npm run build && firebase deploy",
    "prepare": "husky install"
  },
